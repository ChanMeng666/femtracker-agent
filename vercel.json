{
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/copilotkit",
      "destination": "/api/copilotkit"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
} 